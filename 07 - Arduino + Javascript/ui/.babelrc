{
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        "transform-es2015-modules-commonjs"
    ],
    "presets": ["es2015", ["env", { "modules": false }], "stage-3"]
}
